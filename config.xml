<?xml version='1.0' encoding='utf-8'?>
<widget id="com.duchuyctlk.phonegap.taptoclean" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TapToCleanPhoneGap</name>
	<preference name="Orientation" value="portrait" />
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="Notification">
    	<param name="android-package" value="org.apache.cordova.Notification" />
	</feature>
	<feature name="Device">
	    <param name="android-package" value="org.apache.cordova.device.Device" />
	</feature>
	<feature name="WebDatabase">
        <param name="wp-package" value="WebDatabase" />
    </feature>
</widget>
